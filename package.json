{
  "name": "thread-research",
  "version": "2.0.0",
  "description": "A healthcare application",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "lint:fix": "eslint --fix --ext .js,.vue src tests/unit/specs tests/e2e/specs",
    "build:storybook": "vue-cli-service build:storybook -c config/storybook",
    "dev": "vue-cli-service serve --open",
    "serve:storybook": "vue-cli-service serve:storybook -p 6006 -c config/storybook",
    "test:e2e": "vue-cli-service test:e2e",
    "test:unit": "vue-cli-service test:unit",
    "precommit": "lint-staged && npm run test:unit"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^5.13.0",
    "@opentok/client": "^2.15.11",
    "apexcharts": "^3.21.0",
    "async": "^2.6.1",
    "autoprefixer": "^9.3.1",
    "axios": "^0.18.0",
    "axios-progress-bar": "^1.2.0",
    "babel-plugin-syntax-jsx": "^6.18.0",
    "babel-preset-vue": "^2.0.2",
    "bootstrap": "4.0.0",
    "bootstrap-vue": "^2.0.0-rc.11",
    "chart.js": "^2.9.3",
    "chartjs-chart-matrix": "^0.1.3",
    "chartjs-plugin-annotation": "^0.5.7",
    "cron-builder": "^0.3.0",
    "d3": "^5.16.0",
    "eslint-plugin-html": "^5.0.0",
    "file-saver": "^2.0.0",
    "font-awesome": "^4.7.0",
    "html2canvas": "^1.0.0-rc.5",
    "husky": "^1.2.0",
    "image-to-base64": "^2.1.0",
    "jquery": "^3.3.1",
    "jspdf": "^1.5.3",
    "jwt-decode": "^2.2.0",
    "libphonenumber-js": "^1.0.24",
    "lint-staged": "^8.1.0",
    "lodash": "^4.17.11",
    "lx-pdf": "^0.1.5",
    "moment": "^2.22.2",
    "moment-timezone": "^0.5.23",
    "nprogress": "^0.2.0",
    "papaparse": "^4.6.2",
    "quill": "^1.3.6",
    "signature_pad": "^2.3.2",
    "sinon": "^7.1.1",
    "string-replace-loader": "^2.1.1",
    "sweetalert2": "^7.28.11",
    "timelines-chart": "^2.10.0",
    "toastr": "^2.1.4",
    "vee-validate": "^2.1.1",
    "vue": "^2.5.17",
    "vue-acl": "^4.0.6",
    "vue-apexcharts": "^1.6.0",
    "vue-chartjs": "3.4.0",
    "vue-clipboard2": "^0.3.1",
    "vue-color": "^2.7.0",
    "vue-cropperjs": "^2.2.2",
    "vue-drag-drop": "^1.1.4",
    "vue-i18n": "^8.20.0",
    "vue-instant": "^1.0.2",
    "vue-integer-plusminus": "^1.5.0",
    "vue-loading-overlay": "3.3.4",
    "vue-moment": "^4.0.0",
    "vue-nprogress": "^0.1.5",
    "vue-pdf": "^3.2.2",
    "vue-perfect-scrollbar": "^0.2.1",
    "vue-progressbar": "^0.7.5",
    "vue-quill-editor": "^3.0.6",
    "vue-resource": "^1.5.1",
    "vue-router": "^3.0.1",
    "vue-scrolling-table": "^0.2.2",
    "vue-slider-component": "^2.8.0",
    "vue-switches": "^2.0.1",
    "vue-tel-input": "^2.0.8",
    "vue2-filters": "^0.3.0",
    "vue2-timepicker": "^0.1.4",
    "vuedraggable": "^2.16.0",
    "vuejs-datepicker": "^1.5.4",
    "vuex": "^3.0.1",
    "vuex-router-sync": "^5.0.0",
    "webpack": "4.26.0"
  },
  "devDependencies": {
    "@storybook/addon-actions": "4.0.0-alpha.20",
    "@storybook/addon-links": "4.0.0-alpha.20",
    "@vue/cli-plugin-babel": "^3.1.1",
    "@vue/cli-plugin-e2e-nightwatch": "^3.1.1",
    "@vue/cli-plugin-eslint": "^3.1.5",
    "@vue/cli-plugin-unit-jest": "^3.1.1",
    "@vue/cli-service": "^3.1.4",
    "@vue/eslint-config-standard": "^4.0.0",
    "@vue/test-utils": "^1.0.0-beta.20",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^23.6.0",
    "eslint": "^5.10.0",
    "eslint-plugin-vue": "^5.0.0-0",
    "node-notifier": "^5.2.1",
    "node-sass": "^4.13.1",
    "sass-loader": "^7.0.1",
    "vue-cli-plugin-storybook": "^0.4.8",
    "vue-eslint-parser": "^4.0.3",
    "vue-template-compiler": "^2.5.17"
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "jest": {
    "moduleFileExtensions": [
      "js",
      "jsx",
      "json",
      "vue"
    ],
    "transform": {
      "^.+\\.vue$": "vue-jest",
      ".+\\.(css|styl|less|sass|scss|svg|png|jpg|ttf|woff|woff2)$": "jest-transform-stub",
      "^.+\\.jsx?$": "babel-jest"
    },
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/src/$1"
    },
    "snapshotSerializers": [
      "jest-serializer-vue"
    ],
    "testMatch": [
      "**/tests/unit/**/*.spec.(js|jsx|ts|tsx)|**/__tests__/*.(js|jsx|ts|tsx)"
    ],
    "testURL": "http://localhost/"
  },
  "engines": {
    "nodejs": "10.13.0",
    "npm": "6.4.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run precommit"
    }
  },
  "lint-staged": {
    "*.{js,vue}": [
      "eslint"
    ]
  }
}
